@{
    /**/

    ViewBag.Title = "Share_ConsumableDeduction";
    Layout = "~/Views/Shared/_layout.cshtml";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="~/Areas/Share/JSModule/Share_ConsumableDeduction.js?v1.3"></script>
    <link href="~/Content/css/MenuMaster.css" rel="stylesheet" />
    <style>
        .panel {
            margin-bottom: 2px !important;
            margin-left: 2px !important;
        }

        #ShareCorrrection {
            left: 60px;
            width: 93%;
            position: absolute;
            display: none;
            top: 1px;
        }

        .panel-heading {
            padding: 1px 5px;
        }

        .panel-body {
            padding: 4px 6px;
            margin-top: 1px;
        }

        .selected {
            background-color: #3498db;
            color: #fff;
        }

        .bn {
            margin: 0px;
            font-size: 10pt;
            padding: 0px 12px;
            border-radius: 0;
            height: 32px;
        }

        .btn-add {
            border: none;
            padding: 6px;
            margin-left: 1px;
        }

        .btn {
            padding: 1px 9px;
            font-size: 13px;
            margin-bottom: 2px;
        }

        #tblReoprt thead th {
            padding: 3px;
        }

        #tblReoprt tbody td {
            padding: 3px;
        }

        .panel-heading button {
            float: right;
            margin: -26px 0px;
        }

        .text-white {
            color: #fff;
            margin: 7px 0px;
        }

        #tblDeductionDetails thead th {
            padding: 3px;
        }

        #tblDeductionDetails tbody td {
            padding: 3px;
        }

        #tblExtraDeduction thead th {
            padding: 3px;
        }

        #tblExtraDeduction tbody td {
            padding: 3px;
        }

        .loader {
            margin-left: 86%;
            margin-top: -21px;
            background-color: #d3d0d0;
            border: 4px solid #f3f3f3;
            border-radius: 50%;
            border-top: 4px solid #3498db;
            width: 23px;
            height: 23px;
            -webkit-animation: spin 2s linear infinite;
            animation: spin 2s linear infinite;
        }

        #tblDoctor thead th {
            padding: 3px;
        }
    </style>
</head>
<body>
    <div id="page-content">
        <div class="col-lg-12">
            <div class="row">
                <div class="ml15">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <div class="row">

                                <div class="col-md-2">
                                    <input type="month" id="txtMonth" class="form-control" style="width:70%;margin-top: 5px;margin-bottom: 4px;" />
                                </div>
                                <div class="col-md-4">
                                    <button id="btnReport" onclick="$('#ShareCorrrection').slideToggle('slow')" style="float:left;width: 6%; height: 28px;margin-top: 4px;" class="btn btn-warning"><i class="fa fa-upload">&nbsp;</i></button>
                                  
                                    <div id="ShareCorrrection" style="margin-top:20px" >
                                  
                                        <table>
                                            <tr>
                                                <td>
                                                    <button id="btnDowload" class="btn btn-info bn" onclick="DownloadDoctorShare(this)"><i class="fa fa-download">&nbsp;</i>Download Excel</button>
                                                </td>
                                                <td>
                                                    <input type="file" id="UploadShare" style="width:100px" />

                                                </td>
                                                <td>
                                                    <button id="btnUpload" onclick="UploadShareCorrection(this);"  class="btn btn-warning btn-sm"><i class="fa fa-upload">&nbsp;</i> Upload</button>
                                                </td>
                                            </tr>
                                        </table>
                                        <input type="hidden" id="base64" style="display:none;" />

                                        
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <button id="btnExtra" style="width: 80%; height: 28px;margin-right:-39%;margin-top: 4px;" class="btn btn-danger bn" onclick="GetAllExtraDeduction()"><i class="fa fa-arrow-right">&nbsp;</i>Extra Dedution </button>
                                </div>
                                <div class="col-md-2">
                                    <button id="btnDoctor" style="width: 88%; height: 28px;margin-right: -23%;margin-top: 4px;" class="btn btn-success bn" onclick="GetAllDoctorWise()"><i class="fa fa-download">&nbsp;</i> Doctor Summmary </button>
                                </div>

                                <div class="col-md-2">
                                    <button id="btnCalculator" style="width: 80%; height: 28px; margin-left: 30px;margin-top: 4px;" class="btn btn-warning bn" onclick="GetAllSummmary()"><i class="fa fa-download">&nbsp;</i>Summary</button>
                                </div>
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div style="display:flex">
                                        <button id="btnImport" style="width:25%;height:28px;" class="btn btn-success bn" onclick="SaveImport('ImportData')">
                                            Import (1)
                                            <div id="loadingdata1" class="loader" hidden></div>
                                        </button>
                                        <button id="btnDeduction" style="width:40%;height:28px; margin-left:10px" class="btn btn-warning bn" onclick="SaveImport('AutoDeduct')">
                                            Auto Deduction (2)
                                            <div id="loadingdata2" class="loader" hidden></div>
                                        </button>

                                        <button id="btnSearch" style="width:50%;height:28px;margin-left:10px;" class="btn bg-blue bn" onclick="GetAllDataList()">
                                            Consumable Deduction (3)
                                            <div id="loadingdata3" class="loader" hidden></div>
                                        </button>
                                        <button id="btnCalculator" style="width:39%;height:28px; margin-left:10px;" class="btn btn-primary bn" onclick="SaveCalAndSummarize('Calculate')">
                                            Calculate Share (4)
                                            <div id="loadingdata4" class="loader" hidden></div>
                                        </button>
                                        <button id="btnCalculator" style="width:30%;height:28px; margin-left:10px;" class="btn btn-danger bn" onclick="SaveCalAndSummarize('Summarise')">
                                            Summarize (5)
                                            <div id="loadingdata5" class="loader" hidden></div>
                                        </button>
                                        <button id="btnCalculator" style="width:27%;height:28px; margin-left:10px;" class="btn btn-success bn" onclick="ExcelDownload()">
                                            Download (6)
                                            <div id="loadata6" class="loader" hidden></div>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="table-responsive" style="height:450px;overflow-y:auto;border:1px solid #3498db;margin-top:4px">
                                        <table class="table table-bordered" id="tblReoprt">
                                            <thead>
                                                <tr>
                                                    <th style="width:15%;"> Category</th>
                                                    <th style="width:20%;">Sub Category</th>
                                                    <th style="width:20%;">Remark</th>
                                                    <th style="width:7%;text-align:center">Amount</th>
                                                    <th style="width:7%;text-align:center">Deduction</th>
                                                    <th style="width:7%;text-align:center"></th>
                                                    <th style="width:10%;text-align:center">Deducted</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                    <div style="width:100%; background-color:#b5b1b16b;height:24px;">
                                        <div class="flex">
                                            <div class="col-md-11">
                                                <label style="float:right;margin-right:27px;">Grand Total &nbsp;:</label>
                                            </div>
                                            <div class="col-md-1">
                                                <label id="txttotalAmount" style="text-align:center;font-size:16px;"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade modalProcess" id="GetSummary" tabindex="-1" role="dialog" aria-labelledby="True">
        <div class="modal-dialog modal-md">
            <div class="modal-content" style="margin-top: 20%; width: 180%; margin-left: -18%;">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h5 class="text-white">Deduction Details</h5>
                        <button data-dismiss="modal" class="btn btn-danger btn-xs">x</button>
                    </div>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="section" style="height:175px;">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-responsive" style="height:220px;overflow-y:auto; overflow-x:auto;border:1px solid #337ab7;margin-top: -10px;">
                                            <table class="table table-bordered" id="tblDeductionDetails">
                                                <thead>
                                                    <tr>
                                                        <th style="width:5%;text-align:center">TCount</th>
                                                        <th style="width:8%;text-align:center">Amount</th>
                                                        <th style="width:8%;text-align:center">ItemAmount</th>
                                                        <th style="width:8%;text-align:center">nst_disc</th>
                                                        <th style="width:8%;text-align:center">amt_forshare</th>
                                                        <th style="width:8%;text-align:center">diag_disc</th>
                                                        <th style="width:8%;text-align:center">med_disc</th>
                                                        <th style="width:8%;text-align:center">blood_disc</th>
                                                        <th style="width:8%;text-align:center">ext_cons_discount</th>
                                                        <th style="width:8%;text-align:center">commission_amt</th>
                                                        <th style="width:8%;text-align:center">shareable_amt</th>
                                                        <th style="width:8%;text-align:center">ExTraDeduction</th>
                                                        <th style="width:5%;text-align:center">share_amt</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade modalProcess" id="GetExtra" tabindex="-1" role="dialog" aria-labelledby="True">
        <div class="modal-dialog modal-md">
            <div class="modal-content" style="margin-top: 22%;width: 140%;margin-left: 0%;">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h5 class="text-white">Extra Deduction </h5>
                        <button data-dismiss="modal" class="btn btn-danger btn-xs">x</button>
                    </div>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="section" style="height:175px;">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-responsive" style="height:220px;overflow-y:auto; overflow-x:auto;border:1px solid #337ab7;margin-top: -10px;">
                                            <table class="table table-bordered" id="tblExtraDeduction">
                                                <thead>
                                                    <tr>
                                                        <th style="width:20%">Doctor Id</th>
                                                        <th style="width:20%"><input id="txtdoctorName" placeholder="Doctor Name" class="form-control" style="height: 20px;" /></th>
                                                        <th style="text-align:center;width:13%">Gurantee</th>
                                                        <th style="text-align:center;width:13%">Share Amount</th>
                                                        <th style="text-align:center;width:8%">Deduct</th>
                                                        <th style="text-align:center;width:8%">Cmd</th>
                                                        <th style="text-align:center;width:18%">Extra Deducted</th>

                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>

                                        <div style="width:100%; background-color:#b5b1b16b;height:24px;">
                                            <div class="flex">
                                                <div class="col-md-11">
                                                    <label style="float:right;margin-right:52px;">Grand Total &nbsp;:</label>
                                                </div>
                                                <div class="col-md-1">
                                                    <label id="ExtraTotalAmount" style="text-align:center;font-size:16px;margin-left:-15px;"></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade modalProcess" id="GetDoctor" tabindex="-1" role="dialog" aria-labelledby="True">
        <div class="modal-dialog modal-md">
            <div class="modal-content" style="margin-top: 20%; width: 197%; margin-left: -35%;">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h5 class="text-white">Doctor Wise  Details</h5>
                        <button data-dismiss="modal" class="btn btn-danger btn-xs">x</button>
                    </div>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="section" style="height:175px;">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-responsive" style="height:290px;overflow-y:auto; overflow-x:auto;border:1px solid #337ab7;margin-top: -10px;">
                                            <table class="table table-bordered" id="tblDoctor">
                                                <thead>
                                                    <tr>
                                                        <th style="width:5%;">DoctorId</th>
                                                        <th style="width:20%;"><input id="txtdoctor" class="form-control" placeholder="DoctorName" type="text" style="height:20px;padding: 0px;" /></th>
                                                        <th style="width:5%;text-align:center">TCount</th>
                                                        <th style="width:8%;text-align:center">Amount</th>
                                                        <th style="width:8%;text-align:center">ItemAmount</th>
                                                        <th style="width:8%;text-align:center">nst_disc</th>
                                                        <th style="width:8%;text-align:center">amt_forshare</th>
                                                        <th style="width:8%;text-align:center">diag_disc</th>
                                                        <th style="width:8%;text-align:center">med_disc</th>
                                                        <th style="width:8%;text-align:center">blood_disc</th>
                                                        <th style="width:8%;text-align:center">ext_cons_discount</th>
                                                        <th style="width:8%;text-align:center">commission_amt</th>
                                                        <th style="width:8%;text-align:center">shareable_amt</th>
                                                        <th style="width:8%;text-align:center">ExTraDeduction</th>
                                                        <th style="width:5%;text-align:center">share_amt</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>


