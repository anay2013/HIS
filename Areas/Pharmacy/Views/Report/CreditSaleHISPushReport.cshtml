
@{
    ViewBag.Title = "CreditSaleHISPushReport";
    Layout = "~/Views/Shared/_layout.cshtml";
}
<style>
    #ddlFilterBy {
        border-radius: 0;
        margin-right: 14px;
        border-color: #00a65a;
        width: 45%;
    }

    #txtSearchIPDNO {
        border-right: none;
        width: 45%;
        border-color: #00a65a;
        border-radius: 0;
    }

    div#skill .circle {
        position: relative;
        height: 25px;
        width: 25px;
        border: 3px solid #ff934c;
        border-top-color: #e60000;
        border-radius: 50%;
        animation: spin 2s linear infinite;
        top: -10px;
        left: -5px;
    }

    div#skill {
        /* background-color: rgba(144, 0, 64, 0.8); */
        color: #fff;
        padding: 9px 0;
        position: absolute;
        display: none;
    }

    .btn-close {
        right: 5px;
        opacity: 100%;
        position: absolute;
        color: #fff !important;
        font-weight: bold;
        font-size: 12px;
        padding: 0px 6px;
        background: #b92c02;
        height: 16px;
        border-radius: 2px;
        cursor: pointer;
    }

    .flex {
        display: flex !important;
        align-items: center;
    }

    hr {
        margin-top: 35px;
        margin-bottom: 5px;
        border: 0;
        border-top: 1px solid #eee;
    }

    .custom-table {
        width: 100%;
        line-height: 29px;
        font-size: 12px;
    }

    .fileinput.input-group {
        display: flex;
    }

    .text-17 {
        font-size: 17px;
    }

    .btn-flat {
        padding: 2px;
        border: none;
    }

    .scroll {
        width: 100%;
        background: #fff;
        overflow: overlay;
        max-height: 450px;
        display: block;
    }

    .panel-body {
        /*box-shadow: 1px 1px 10px #d2d2d2;*/
        border-radius: 10px;
        padding: 5px 5px 0px 10px;
        position: relative;
    }

    .note {
        list-style: disc;
        color: red;
        font-size: 11px;
        font-weight: bold;
        background: #f7f7f7;
        border: 1px solid #d86c6c;
    }

    .billrecpt {
        font-size: 12px;
        color: #005aff;
        font-weight: bold;
    }

    #tblIPDInfo td {
        font-size: 13px;
        color: #242222;
    }
</style>
<div id="page-content">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="ml15">
                    <div class="">
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


