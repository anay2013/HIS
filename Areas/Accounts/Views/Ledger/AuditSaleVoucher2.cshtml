@{
    ViewBag.Title = "AuditSaleVoucher";
    Layout = "~/Views/Shared/_layout.cshtml";
}
<script src="~/Areas/Accounts/JsModule/AuditSaleVoucher.js"></script>
<style>

    .panel {
        margin-bottom: 2px !important;
        margin-left: 2px !important;
    }

    .panel-heading {
        padding: 1px 5px;
    }

    .panel-body {
        padding: 2px 1px;
        margin-top: 1px;
    }


    #tblSaleVoucher thead th {
        color: #fff;
        background: #2478a9;
        font-size: 12px;
        font-weight: bold;
        padding-right: 3px;
        text-align: center;
    }

    #tblSaleVoucher tbody td {
        font-size: 14px;
        font-weight: bold;
        padding-right: 10px;
        text-align: center;
    }

    .row.colnospace [class*='col-'] {
        padding-right: 2px;
        padding-left: 2px;
    }

    .nav-tabs a {
        background: #e8e8e8;
    }

    .tab-pane {
        padding: 2px;
    }

    .btn {
        border: none;
        padding: 1px 9px;
        font-size: 13px;
        margin-bottom: 2px;
    }

    .navigation div[class*=col-md-3] {
        width: 25%;
        /*//padding: 2px;*/
    }

    /*.navigation div[class*=col-md-3] {
        width: 38%;
       padding: 2px;
    }*/

    .navigation input {
        padding: 1px;
    }

    .bn {
        margin: 0px;
        font-size: 10pt;
        padding: 0px 12px;
        border-radius: 0;
        height: 32px;
        /*margin-top: 11%;*/
    }

    .panel-heading button {
        float: right;
        margin: -20px 0px;
    }

    .text-white {
        color: #fff;
        margin: 7px 0px;
    }

    #tblSaleVoucherInfo thead th {
        font-size: 14px;
        font-weight: bold;
        color: #fff;
        background: #2478a9;
    }

    #tblSaleVoucherInfo tbody td {
        font-size: 12px;
        /*color: #fff;*/
    }
</style>
<div id="page-content">
    <div class="container">
        <div class="ml15">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h5>Audit Sale Voucher</h5>
                </div>
                <div class="panel-body">

                    <div class="row navigation" style="margin-left: -2px;">
                        <div class="col-md-3">
                            From
                            <input type="date" id="txtfromdate" class="form-control" />
                        </div>
                        <div class="col-md-3">
                            To
                            <input type="date" id="txttodate" class="form-control" />
                        </div>

                        <div class="col-md-3">
                            <div style="display:flex">
                                <button id="btnGet" style="width:25%;height: 28px; margin-left:8px;margin-top:7%" class="btn btn-warning bn" onclick="GetDataAllBindNew()"><i class="fa fa-sign-in">&nbsp;</i>Get</button>
                                <button id="btnExecl" style="width:25%; height: 28px;margin-left:8px;margin-top:7%" class="btn btn-success bn" onclick="DownloadExcelAuditSale()"><i class="fa fa-file-excel-o">&nbsp;</i>Excel</button>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="panel-body">
                    <div class="row colnospace">
                        <div class="scroll" style="margin-bottom: 5px;overflow:auto;border: 1px solid #2478a9;margin-left:18px;  margin-right:18px;">
                            <table id="tblSaleVoucher" class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th>CMD</th>
                                        <th>Date</th>
                                        <th>Total Sales</th>
                                        <th>By Cash</th>
                                        <th>By Neft</th>
                                        <th>By Swipe Card</th>
                                        <th>Advance Adj Amount</th>
                                        <th>Receivable</th>
                                        <th>Vch Amount</th>
                                        <th>Diff</th>
                                        <th>Command</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>

                    </div>
                </div>

                <div class="modal fade modalProcess" id="SaleVoucherDetailsPop" tabindex="-1" role="dialog" aria-labelledby="True">
                    <div class="modal-dialog modal-md">
                        <div class="modal-content" style="margin-top:10%; width:190%; margin-left:-35%;">
                            <div class="panel panel-primary">
                                <div class="panel-heading">
                                    <h5 class="text-white">Audit Sale Voucher Details</h5>
                                    <button data-dismiss="modal" class="btn btn-danger btn-xs">x</button>

                                </div>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="section" style="height:175px;">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="table-responsive" style="height:300px;overflow-y:auto; overflow-x:auto;border:1px solid #337ab7;margin-top: -10px;">
                                                        <table class="table table-bordered" id="tblSaleVoucherInfo">
                                                            <thead>
                                                                <tr>

                                                                    <th style="text-align:center;width:10%">LedgerId</th>
                                                                    <th style="text-align:left;width:30%">LedgerName</th>
                                                                    <th style="text-align:center;width:10%">ReceiptType</th>
                                                                    <th style="text-align:center;width:5%">SeqNo</th>
                                                                    <th style="text-align:center;width:10%">RefNo</th>
                                                                    <th style="text-align:center;width:10%">VchAmt</th>
                                                                    <th style="text-align:center;width:10%">Amount</th>
                                                                    <th style="text-align:left;width:15%">Narration</th>

                                                                </tr>
                                                            </thead>
                                                            <tbody></tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

