    }

    .basic {
        background: #0cdf7e6b;
    }

        .basic:nth-child(1) {
            border-top: 5px solid white;
            border-bottom: 5px solid white;
        }

    .request {
        background: #f7e0e0;
    }

        .request:nth-child(1) {
            border-top: 5px solid white;
