                                            <td><b>Patient Name</b></td>
                                            <td><b>:</b></td>
                                            <td id="txtPatientName"></td>
                                        </tr>
                                        <tr>
                                            <td><b>Age</b></td>
                                            <td><b>:</b></td>
                                            <td id="txtAge"></td>
                                            <td>&nbsp;</td>
                                            <td><b>Gender</b></td>
                                            <td><b>:</b></td>
                                            <td id="txtGender"></td>
                                        </tr>
                                        <tr>
