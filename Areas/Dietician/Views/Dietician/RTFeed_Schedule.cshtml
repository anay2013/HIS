    .panel-body {
        /*box-shadow: 1px 1px 10px #d2d2d2;*/
        border-radius: 10px;
        padding: 5px 3px 0px 3px;
        position: relative;
    }

    .note {
        list-style: disc;
        color: red;
        font-size: 11px;
        font-weight: bold;
        background: #f7f7f7;
        border: 1px solid #d86c6c;
    }

    .bn {
        margin: 0px;
        font-size: 10pt;
        padding: 0px 10px;
        border-radius: 0;
        text-align: left;
        height: 30px;
    }

    .bc {
        border: 1px solid #00a65a;
        border-radius: 0;
    }

    .section-feedback {
        border-top: 2px solid #f39c12;
        background: #fff;
        padding: 5px;
        border-radius: 2px;
        margin: -12px 0px;
    }

    .section {
        border-top: 2px solid #0076d0;
        background: #fff;
        padding: 5px;
        border-radius: 2px;
        margin: 9px 0px;
    }

        .section table {
            margin: 0;
        }

            .section table th {
                font-weight: bold;
            }

            .section table th, .section table td {
                font-size: 12px;
            }

    h3.modal-schedule {
        margin: 0;
        background: #2492b3;
        color: #ffffff;
        padding: 4px;
        font-size: 16px;
        border-bottom: 1px solid white;
    }

    #tblSchedule, #tblStatus {
        margin: 0;
        border: 1px solid #2492b3;
        min-height: 130px;
        max-height: 130px;
    }

    .hide {
        display: none;
    }

    .bcc {
        border: 1px solid #f39c12;
        border-radius: 0;
    }

    .btnRate {
        height: 28px;
        width: 30%;
        border-radius: 0;
        margin: 0;
    }

    .patient-list {
        border: 1px solid #e9e9e9;
        margin: 4px 0;
        background: #f3f3f3;
        padding: 3px;
    }

    #tblInfo tr th, #tblInfo tr td {
        padding: 1px;
        color: #4b5056;
        background-color: #d4ecfd;
    }

    .patient-list label {
        font-size: 11px;
    }

    .ratecircle {
        margin: 5px;
        border: 2px solid #2f89e7;
        color: #302f2f;
        width: 30px;
        height: 30px;
        background: #eaffff;
        border-radius: 40px;
        line-height: 27px;
        text-align: center;
        font-size: 19px;
        font-weight: bold;
        cursor: pointer;
    }

    .ratecircle-head {
        border: 2px solid #e7c82f;
        color: #ffffff;
        width: 30px;
        height: 30px;
        background: #ff2d2d;
        border-radius: 40px;
        line-height: 27px;
        text-align: center;
        font-size: 19px;
        font-weight: bold;
        cursor: pointer;
    }

    #tblQuest thead th p {
        font-size: 9px;
        text-align: center;
        margin: 0;
    }

    #tblQuest tbody th, #tblQuest tbody td {
        vertical-align: inherit;
    }

    .Inactive {
        /*opacity: 0.5;*/
        pointer-events: none;
        cursor: not-allowed;
    }

    .overAllRate {
        width: 40px;
        background: #1569e7;
        height: 40px;
        border-radius: 30px;
        font-size: 18px;
        text-align: center;
        padding: 4px;
    }

    #IPDPatientList {
        background: #ddd;
        min-height: 90%;
        max-height: 100%;
        overflow: auto;
        padding: 0px 5px;
    }

    span1 {
        font-size: 13px;
        font-weight: bold;
    }

    .dischargeInfo {
        background: #f3d2d2;
        width: 100%;
        padding: 0px 3px;
    }

    .pfeedbackList {
        height: 90%
    }


    #totaFilterCount {
        color: #539906;
    }

    #patientInfo tbody td {
        border: none;
        font-size: 12px;
        padding: 4px 0px;
    }

    .patientInfo {
        border: 1px solid #efefef;
        background: #e5f2ff;
        padding: 2px 10px;
        margin: -2px 10px;
        width: -webkit-fill-available;
    }

    .panel-heading button#btndetails {
        float: right;
        margin: -16px 2px;
    }

    .text-white1 {
        color: #fff;
        margin: -3px 0px;
    }

    .sectionid {
        /* border-top: 2px solid #0076d0; */
        background: #fff;
        padding: 5px;
        border-radius: 2px;
        margin: 9px 0px;
    }
</style>
<div id="page-content">
    <div class="container">
        <div class="ml15">


            <div class="row">
                <div class="col-md-5 pfeedbackList">
                    <div class="panel">
                        <div class="panel-heading" style="background:#0076d0;padding: 4px;">
                            <h4 class="text-white text-center">IPD Patient Info</h4>

                        </div>
                        <div class="panel-body">
                            <div class="col-xs-3">
                                <label>Date</label>
                                <input type="date" id="txtInputDate" onchange="GetPatientDetails()" class="form-control" />
                            </div>
                            <div class="col-xs-3">
                                <label>Filter</label>
                                <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                    <input type="text" id="txtSearchPatient" class="form-control input_pass" value="" placeholder="Search...">
                                </div>
                            </div>
                            <div class="col-xs-3">
                                <label>Floor</label>
                                <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                    <select class="form-control" id="ddlRoom"></select>
                                </div>
                            </div>
                            <div class="col-xs-3">
                                <label>Room <span id="totaFilterCount"></span></label>
                                <div class="fileinput fileinput-new input-group flex">
                                    <select class="form-control" id="ddlRoomType"></select>
                                    <label class="NewLabel"><input type="checkbox" name="New" value="New" />New</label>
                                </div>
                            </div>

