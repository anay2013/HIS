    }

    #patientInfo tbody td {
        border: none;
        font-size: 12px;
        padding: 4px 0px;
    }

    .patientInfo {
        border: 1px solid #efefef;
        background: #fff8e5;
        padding: 5px;
        margin-bottom: 10px;
    }

    .selected {
