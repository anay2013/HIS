
@{
    /**/

    ViewBag.Title = "TotalPatientReport";
    Layout = "~/Views/Shared/_layout.cshtml";
}

<link href="~/Content/css/MenuMaster.css" rel="stylesheet" />
<script src="~/Areas/OPD/JsModule/TotalPatientReport.js"></script>

<style>
    .opdCount {
        position: absolute;
        /*top: 11.7%;*/
        top: 7.7%;
        left: 79%;
        z-index: 10;
        border-radius: 35px;
        padding: 1px 3px;
    }

        .opdCount Total, .opdCount Pending, .opdCount Consultant {
            color: #fff;
            display: ruby;
            background: #259dc3;
            border-radius: 30px;
            border: 1px solid #ddd;
            margin: 0px 1px;
            padding: 1px 9px;
            font-size: 12px;
        }

   
    .select-row {
        color: #fff !important;
    }

    #GatewayIframe {
        display: none;
        position: fixed;
        top: 0;
        left: 0;
    }

    .openFrame {
        z-index: 10000;
        display: block !important;
        width: 100% !important;
        height: 100% !important;
    }

 
    .size-xs, .btn-xs, .btn-group-xs > .btn {
        font-size: 11px;
        line-height: 14px;
        min-width: 22px;
        height: 16px;
    }

    .btnRound {
        margin-left: 3px;
        border-radius: 30px;
    }

    #rolePanel .panel-body, #roleAllotedPanel .panel-body, #divisionAllotmentPanel .panel-body, #unitAllotedPanel .panel-body {
        background: #fbfbfb;
        padding: 0px;
        list-style: none;
        border: 1px solid #e6e6e6;
        max-height: 330px;
        overflow-y: auto;
        overflow-x: hidden;
    }

    .col-md-2 {
        width: 13%;
        padding: 0 2px;
    }

    .panel-title {
        font-size: 14px;
        margin-top: 0;
        margin-bottom: 0;
        color: inherit;
    }

    .panel-heading {
        padding: 2px 10px;
        border-bottom: 1px solid transparent;
    }

    .row-green {
        background: #378a45;
        color: #fff;
    }

    .slot-selected {
        background: green !important;
    }

    .timeChoose {
        padding: 3px;
        color: #fff;
        background: #842407;
        border: 1px solid #ddd;
    }

    .TimeFrame {
        max-height: 480px;
        overflow: auto;
        overflow-y: auto;
    }

    .TimeSlot-Status {
        padding: 2px;
        color: white;
        background: #064c67;
        border-radius: 5px;
    }

    .TimeSlot-Title {
        align-items: center;
        display: flex;
        border: 1px solid #ddd;
        background: #f1f1f1;
        padding: 4px 6px;
        font-size: 13px;
        font-weight: bold;
        color: #036f98;
    }

    .TimeSlot {
        display: -webkit-inline-box;
        width: 18%;
        margin: 6px 7px;
        box-shadow: 1px 2px 4px #ddd;
        cursor: pointer;
        background: #cccccc;
        color: #fff;
        padding: 2px 22px;
        /* border-top-left-radius: 15px; */
        /* border-bottom-right-radius: 15px; */
        line-height: 22px;
        text-align: center;
    }

        .TimeSlot:hover {
            box-shadow: 1px 2px 4px #4c4646;
            transition: all 0.6s;
        }

    span[disabled] {
        background: #ccc !important;
        cursor: not-allowed !important;
    }

        span[disabled] > .TimeSlot-Status {
            background: #595a58 !important;
        }

    .disabled {
        background: #ddd !important;
    }

    .btn-flat {
        padding: 5px;
        border: navajowhite;
    }

    .search-section {
        border: 1px solid #ddd;
        background: #ffeaea;
        padding: 5px;
        line-height: 26px;
        text-align: center;
    }

    .flex {
        display: flex !important;
        align-items: center;
    }

    hr {
        margin-top: 35px;
        margin-bottom: 5px;
        border: 0;
        border-top: 1px solid #eee;
    }

    .fileinput.input-group {
        display: flex;
    }

    .text-17 {
        font-size: 17px;
    }

    .scroll {
        width: 100%;
        background: #fff;
        overflow: overlay;
        max-height: 480px;
        display: block;
    }

    .panel-body {
        /*box-shadow: 1px 1px 10px #d2d2d2;*/
        border-radius: 10px;
        padding: 5px 3px 0px 3px;
        position: relative;
    }

    .ml15 {
        margin-left: 15px;
    }

    .btn-close {
        right: 5px;
        opacity: 100%;
        position: absolute;
        color: #fff !important;
        font-weight: bold;
        font-size: 12px;
        padding: 0px 6px;
        background: #b92c02;
        height: 16px;
        border-radius: 2px;
        cursor: pointer;
    }

    .Vital-title {
        padding-bottom: 5px;
        border: 1px solid #404040;
        text-align: center;
        color: #000;
        font-family: Verdana, Arial, sans-serif;
        font-weight: bold;
    }

    .category {
        font-weight: bold;
        background: #2478a9;
        display: block;
        color: #ffffff;
        padding: 3px 9px;
    }

    .note {
        border: none;
        width: 38px;
        height: 25px;
        margin: 0 5px;
        float: left;
        border-radius: 2px;
    }

        .note i {
            color: #fff;
        }

        .note:hover {
            background: linear-gradient(to bottom right, #007eff, #615959) !important;
        }

    .table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
        padding: 2px 2px 0px 3px;
        border-top-width: 1px;
        border-top-style: solid;
        text-align: left;
        font-size: 14px;
        color: black;
    }
</style>
<div id="page-content">
    <div class="opdCount"><Total></Total><Consultant></Consultant><Pending></Pending></div>
    <div class="container">
        <div class="ml15">
            <div class="panel-body">
                <div class="row navigation" style="background-color:#bddeff;margin-bottom:10px;height:60px;padding-top: 5px;">
                    <div class="col-md-3">
                        Doctor Name
                        <select class="form-control" id="ddlDoctor">
                            <option value="Prompt">Select Doctor</option>
                        </select>
                    </div>
                    <div class="col-md-2">
                        From Date
                        <input type="date" id="txtFromAppDate" class="form-control" placeholder="from : dd-mm-yyyy" />
                    </div>
                    <div class="col-md-2" style="width:17%">
                        To Date
                        <div style="display:flex">
                            <input type="date" id="txtToAppDate" class="form-control" placeholder="to : dd-mm-yyyy" />
                            <input type="button" id="btnSearchByDate" onclick="ViewConsultation('TotalPatientReport')" class="btn-success btn-flat" value="Search" />
                        </div>
                    </div>

                </div>
                <div class="row">
                    <div class="col-md-12" style="padding:0">
                        <span class="category">Patient Report</span>
                        <div class="table-responsive" style="height:400px;overflow-y:auto; overflow-x:auto;border:1px solid #337ab7;margin-top:3px;">
                            <table class="table table-bordered" id="tblReport">
                                <thead>
                                    <tr>

                                        <th>Token</th>
                                        <th>UHIDNO</th>
                                        <th>App. No</th>
                                        <th>Doctor Name</th>
                                        <th><input type="text" id="txtseachEmer" onkeyup="searchTable('txtseachEmer','tblReport')" class="form-control" placeholder="search by name.." style="height:22px;margin-bottom:2px;" /></th>
                                        <th>AgeInfo</th>
                                        <th>Mobile. No</th>
                                        <th>Date</th>
                                        <th>Status</th>
                                    
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<iframe id="GatewayIframe" src="#" class="hide1"></iframe>





