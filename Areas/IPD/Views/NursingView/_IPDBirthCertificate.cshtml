
<script src="~/Areas/IPD/JsModule/NursingView/_IPDBirthCertificate.js"></script>
<style>
    .select2-container {
      
        position: relative;
       
    }
    .col-md-3, .col-md-4 {
        padding: 0;
    }
    .select2-container .select2-selection--single .select2-selection__rendered {
        display: block;
        padding-left: 0px;
        padding-right: 20px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
    }
    .example-box-wrapper {
        margin: 10px 20px;
    }
</style>
<div class="example-box-wrapper">

    <div class="row" style="border:1px solid black;text-align:center; padding:1px; margin-right: 0px; margin-left: 0px;">
        <div class="col-md-12">
            <label style="font-size:15px"><b>Birth Certificate Entery </b></label>
        </div>
    </div>
    <div class="row" style="border:1px solid black; padding:15px; margin-right: 0px; margin-left: 0px;margin-top: 1px;">
        <div class="col-md-12">
            <table style="width:100%;font-size:15px">
                <tr>
                    <td style="text-align:right;margin-left:10px;font-weight: bold;"><b>Date Of Issue</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;"><input id="txtdateIssue" style="height:23px;padding:0px;font-size:12px" type="date" class="form-control"></td>
                    <td>&nbsp;</td>
                    <td style="text-align:right;margin-left:10px"><b>Delivery Type</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;">
                    <select id="ddlDeliveryType" class="form-control" style="font-size:12px">
                        <option value="0">Select</option>
                        <option value="VBAC">VBAC</option>
                        <option value="C-section">C-section</option>
                        <option value="Vaginal_Delivery">Vaginal_Delivery</option>
                        </select></td>
                </tr>

                <tr>
                    <td style="text-align:right;margin-left:10px"><b>Date/Time Of Delivery</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td>
                        <input id="txtdatetime" style="height:23px;padding:0px;font-size:12px" type="datetime-local" class="form-control">
                    </td>
                    <td>&nbsp;</td>

                    <td style="text-align:right;margin-left:10px;font-weight: bold;"><b>Guardian Name</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;"><input id="txtguardian" style="height:23px;padding:0px;font-size:12px" type="text" class="form-control"></td>
                </tr>

                <tr>
                    <td style="text-align:right;margin-left:10px"><b>Baby Name</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;font-size:12px">  <input id="txtbabyname" style="height:23px;padding:0px;font-size:12px" type="text" class="form-control"></td>
                    <td>&nbsp;</td>
                    <td style="text-align:right;margin-left:10px"><b>Gender</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;font-size:12px">
                        <div>
                            <input type="radio" id="ddlGender" name="Gender" value="Male">
                            <label for="Gender"> Male </label>
                            <input type="radio" id="ddlGender" name="Gender" value="Female">
                            <label for="Gender"> Female </label>

                        </div>
                    </td>
                </tr>

                <tr>
                    <td style="text-align:right;margin-left:10px;font-weight: bold;"><b>Height</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;"><input id="txtheight" style="height:23px;padding:0px;font-size:12px" type="text" class="form-control"></td>
                    <td>&nbsp;</td>
                    <td style="text-align:right;margin-left:10px"><b>Weight</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;"><input id="txtWeight" style="height:23px;padding:0px;font-size:12px" type="text" class="form-control"></td>
                </tr>
                <tr>
                    <td style="text-align:right;margin-left:10px"><b>Address</b></td>
                    <td style="margin-left:10px;width:20px"><b>:</b></td>
                    <td style="text-align:left;margin-left:10px;padding-bottom: 5px;" colspan="5"><textarea id="textaddress"  style="height:50px;padding:0px;font-size:12px" class="form-control" type="text" ></textarea></td>
                </tr>


            </table>
        </div>
    </div>

    <div class="row" style="border:1px solid black;text-align:center; padding:1px; margin-right: 0px; margin-left: 0px;margin-top:1px">
        <div class="col-md-12">
            <label id="txtautoid" hidden></label>
            <button class="btn btn-success" style="width: 7%;height:27px;margin:0;border-radius:0;padding: 0 2px;margin-top: 2px; margin-bottom: 2px;" onclick="Savedata()" id="btnsave"><i class="fa fa-sign-in">&nbsp;</i>Save</button>
            <button class="btn btn-warning" style="width: 7%;height:27px;margin:0;border-radius:0;padding: 0 2px;margin-top: 2px; margin-bottom: 2px; display:none" onclick="Updatedata()" id="btnupdate"><i class="fa fa-sign-in">&nbsp;</i>Update</button>
            <button class="btn btn-success" style="width: 7%;height:27px;margin:0;border-radius:0;padding: 0 2px;margin-left:5px;margin-top: 2px;margin-bottom: 2px" onclick="PrintBirthCertificate()"><i class="fa fa-print">&nbsp;</i>Print</button>
        </div>
    </div>
</div>
