<script src="~/Areas/IPD/JsModule/NursingView/_DeltaReport.js"></script>


<style>
    .example-box-wrapper {
        margin: 10px 10px;
    }

    .selected {
        background: DodgerBlue !important;
        color: #fff;
    }

    .panel-body {
        /*padding: 15px 15px 0px 20px;*/
        padding: 5px 3px 0px 3px;
        position: relative;
    }
    hr {
        /*margin-top: 35px;*/
        margin-bottom: -10px;
        border: 0;
        border-top: 1px solid #eee;
    }

    .example-box-wrapper {
        /*margin: 10px 10px;*/
        margin: 2px 10px;
    }
    #tblDeltaReport thead {
        font-size: 12px;
        font-weight: bold;
        color: #fff;
    }
    .table-responsive {
        min-height: .01%;
        overflow-x: auto;
    }
    .scroll {
        width: 100%;
        background: #fff;
        overflow-x: auto;
        height: 420px;
        display: block;
        border: 1px solid #2478a9;
    }
</style>
<div class="example-box-wrapper">
    <div class="row">
        <div class="col-md-4">

            <div class="row">
                <div class="col-md-5">
                    <label>From</label>
                    <input type="date" class="form-control" id="txtfromdate" />
                </div>
                <div class="col-md-5">
                    <label>To</label>
                    <input type="date" class="form-control" id="txtTodate" />
                </div>
                <div class="col-md-2">
                    <button class="btn-success" type="submit" id="btnGet" style="border:none;height:25px;margin-top:60%;margin-left:-3px;"><i class="fa fa-search">&nbsp;</i>Get</button>
                </div>
            </div>
            <div class="table-responsive" style="height:420px;overflow-y:auto; overflow-x:auto;border:1px solid #2478a9;margin-top:5px;">
                <table id="tblDeltaReport" class="table table-bordered" style="margin-bottom:6px">
                    <thead>
                        <tr>


                            @*<th style="width:20%;text-align:center">TestCode</th>*@
                            <th style="width:30%;text-align:left">TestName</th>
                            <th style="width:20%;text-align:left">LastDate</th>
                            <th style="width:20%; text-align:center">Count</th>
                            <th style="width:10%;text-align:center">CMD</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
        <div class="col-md-8">
            <div class="row">
                <div class="col-md-12">
                    <div class="panel-heading" style="background:#dff3ff;margin-bottom:3px;margin-right:7px;">
                        <div class="row">
                            <div class="col-md-10">
                              <label style="width:13%;margin-top: 4%; margin-left: 10px;">Test Name :</label>
                              <label style="margin-top: 4%;" id="txttestname"></label>

                            </div>
                            <div class="col-md-2 pull-right">
                                <button class="btn-warning" type="submit" id="btnPrint" style="border:none;height:25px;margin-top:25%; margin-left:53px;" onclick="DeltaReportPrint()"><i class="fa fa-print">&nbsp;</i>Print</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                        <div id="divDelta" class="table-responsive" style="height:420px;overflow-y:auto; overflow-x:auto;border:1px solid #2478a9;width:99%">
                     
                    </div>
                    </div>
                </div>

            </div>
        </div>   
     </div>
